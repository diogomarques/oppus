            android:key="@string/key_t_scan"
            android:title="t_scan" />
        <EditTextPreference
            android:defaultValue="@string/value_t_con"
            android:dialogMessage="@string/t_con_title"
            android:key="@string/key_t_con"
            android:title="t_con" />
         <EditTextPreference
            android:defaultValue="@string/value_t_int"
            android:dialogMessage="@string/t_int_title"
            android:key="@string/key_t_int"
            android:title="t_int" />
        
        <CheckBoxPreference
